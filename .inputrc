set editing-mode vi
set completion-ignore-case on
set show-all-if-ambiguous on
set show-all-if-unmodified on
set menu-complete-display-prefix on

# This will have it cycle through options, but won't print out all possibilities
# TAB: menu-complete
# bind shift-tab to reverse menu complete
"\e[Z": menu-complete

# Allows you to start typing and then search in your history
# for previously typed lines with that command. Very similiar to ctrl+r
"\e[A": history-search-backward
"\e[B": history-search-forward

set keymap vi-insert
"\C-L": clear-screen
